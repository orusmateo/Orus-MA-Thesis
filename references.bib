@article{cesnavicius_purity_2024,
  title = {Purity for Flat Cohomology},
  author = {{\v C}esnavi{\v c}ius, K{\k e}stutis and Scholze, Peter},
  year = {2024},
  month = jan,
  journal = {Annals of Mathematics},
