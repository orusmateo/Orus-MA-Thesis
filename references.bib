@misc{zhao_survey_2024,
  title = {A {{Survey}} of {{Large Language Models}}},
  author = {Zhao, Wayne Xin and Zhou, Kun and Li, Junyi and Tang, Tianyi and Wang, Xiaolei and Hou, Yupeng and Min, Yingqian and Zhang, Beichen and Zhang, Junjie and Dong, Zican and Du, Yifan and Yang, Chen and Chen, Yushuo and Chen, Zhipeng and Jiang, Jinhao and Ren, Ruiyang and Li, Yifan and Tang, Xinyu and Liu, Zikang and Liu, Peiyu and Nie, Jian-Yun and Wen, Ji-Rong},
  year = {2024},
  month = sep,
  number = {arXiv:2303.18223},
  eprint = {2303.18223},
  primaryclass = {cs},
  publisher = {arXiv},
  doi = {10.48550/arXiv.2303.18223},
  urldate = {2024-09-29}, note = {{(Accessed 2024 September 29)}},
  abstract = {Language is essentially a complex, intricate system of human expressions governed by grammatical rules. It poses a significant challenge to develop capable AI algorithms for comprehending and grasping a language. As a major approach, language modeling has been widely studied for language understanding and generation in the past two decades, evolving from statistical language models to neural language models. Recently, pre-trained language models (PLMs) have been proposed by pre-training Transformer models over large-scale corpora, showing strong capabilities in solving various NLP tasks. Since researchers have found that model scaling can lead to performance improvement, they further study the scaling effect by increasing the model size to an even larger size. Interestingly, when the parameter scale exceeds a certain level, these enlarged language models not only achieve a significant performance improvement but also show some special abilities that are not present in small-scale language models. To discriminate the difference in parameter scale, the research community has coined the term large language models (LLM) for the PLMs of significant size. Recently, the research on LLMs has been largely advanced by both academia and industry, and a remarkable progress is the launch of ChatGPT, which has attracted widespread attention from society. The technical evolution of LLMs has been making an important impact on the entire AI community, which would revolutionize the way how we develop and use AI algorithms. In this survey, we review the recent advances of LLMs by introducing the background, key findings, and mainstream techniques. In particular, we focus on four major aspects of LLMs, namely pre-training, adaptation tuning, utilization, and capacity evaluation. Besides, we also summarize the available resources for developing LLMs and discuss the remaining issues for future directions.},
  archiveprefix = {arXiv},
  keywords = {Computer Science - Artificial Intelligence,Computer Science - Computation and Language},
  file = {/Users/mateocastano-suarez/Zotero/storage/N5DQIQM7/Zhao et al_2024_A Survey of Large Language Models.pdf;/Users/mateocastano-suarez/Zotero/storage/XS3PUQH9/2303.html}
}

@article{zielasko_interactive_2016,
  title = {Interactive {{3D Force-Directed Edge Bundling}}},
  author = {Zielasko, Daniel and Weyers, Benjamin and Hentschel, Bernd and Kuhlen, Torsten Wolfgang},
  year = {2016},
  month = jun,
  journal = {Computer Graphics Forum},
  volume = {35},
  number = {3},
  pages = {51--60},
  issn = {1467-8659},
  doi = {10.1111/cgf.12881},
  urldate = {2024-08-22}, note = {{(Accessed 2024 August 22)}},
  abstract = {Interactive analysis of 3D relational data is challenging. A common way of representing such data are node-link diagrams as they support analysts in achieving a mental model of the data. However, na{\"i}ve 3D depictions of complex graphs tend to be visually cluttered, even more than in a 2D layout. This makes graph exploration and data analysis less efficient. This problem can be addressed by edge bundling. We introduce a 3D cluster-based edge bundling algorithm that is inspired by the force-directed edge bundling (FDEB) algorithm [HvW09b] and fulfills the requirements to be embedded in an interactive framework for spatial data analysis. It is parallelized and scales with the size of the graph regarding the runtime. Furthermore, it maintains the edge's model and thus supports rendering the graph in different structural styles. We demonstrate this with a graph originating from a simulation of the function of a macaque brain.},
  langid = {english},
  file = {/Users/mateocastano-suarez/Zotero/storage/GS934NMJ/Zielasko et al_2016_Interactive 3D Force-Directed Edge Bundling.pdf}
}

@phdthesis{zogheib_paper_2022,
  type = {Thesis},
  title = {The `{{Paper}} and {{Pencil}}' {{Operations}} of {{Interdisciplinarity}}: {{Interdisciplinary Integration}} as {{Information Practice}}},
  shorttitle = {The `{{Paper}} and {{Pencil}}' {{Operations}} of {{Interdisciplinarity}}},
  author = {Zogheib, Ciara},
  year = {2022},
  month = nov,
  urldate = {2024-08-06}, note = {{(Accessed 2024 August 6)}},
  abstract = {Interdisciplinary research produces innovative solutions to complex problems by integrating information from multiple disciplines. At present, despite the positioning of integration as the defining activity of interdisciplinarity, interdisciplinary work tends to be evaluated on the basis of final outcomes, at the expense of a clear understanding of what interdisciplinary integration actually is. This thesis problematizes this outcome focus and makes the case for a practice-focused approach, using conceptual modeling and network analysis of the data holdings of in-progress interdisciplinary research projects to explore how researchers integrate data from multiple disciplines. These case studies demonstrate that even in ostensibly interdisciplinary research, data tend to be analyzed in disciplinary silos, leaving any integration that occurs to take place in an ad-hoc, after-the-fact fashion. The adverse implications of this finding on interdisciplinarity are discussed, and a framework for the repositioning of integration as a research practice is proposed as a potential solution.},
  langid = {english},
  annotation = {Accepted: 2022-11-11T17:45:56Z},
  file = {/Users/mateocastano-suarez/Zotero/storage/7C5PKZJI/Zogheib_2022_The ‘Paper and Pencil’ Operations of Interdisciplinarity.pdf}
}

