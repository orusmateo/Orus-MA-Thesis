@misc{giusti_twos_2016,
  title = {Two's Company, Three (or More) Is a Simplex: {{Algebraic-topological}} Tools for Understanding Higher-Order Structure in Neural Data},
  shorttitle = {Two's Company, Three (or More) Is a Simplex},
  author = {Giusti, Chad and Ghrist, Robert and Bassett, Danielle S.},
  year = {2016},
  month = jan,
  number = {arXiv:1601.01704},
  eprint = {1601.01704},
  primaryclass = {math, q-bio},
  publisher = {arXiv},
  doi = {10.48550/arXiv.1601.01704},
  urldate = {2024-09-15}, note = {{(Accessed 2024 September 15)}},
  abstract = {The language of graph theory, or network science, has proven to be an exceptional tool for addressing myriad problems in neuroscience. Yet, the use of networks is predicated on a critical simplifying assumption: that the quintessential unit of interest in a brain is a dyad -- two nodes (neurons or brain regions) connected by an edge. While rarely mentioned, this fundamental assumption inherently limits the types of neural structure and function that graphs can be used to model. Here, we describe a generalization of graphs that overcomes these limitations, thereby offering a broad range of new possibilities in terms of modeling and measuring neural phenomena. Specifically, we explore the use of {\textbackslash}emph\{simplicial complexes\}, a theoretical notion developed in the field of mathematics known as algebraic topology, which is now becoming applicable to real data due to a rapidly growing computational toolset. We review the underlying mathematical formalism as well as the budding literature applying simplicial complexes to neural data, from electrophysiological recordings in animal models to hemodynamic fluctuations in humans. Based on the exceptional flexibility of the tools and recent ground-breaking insights into neural function, we posit that this framework has the potential to eclipse graph theory in unraveling the fundamental mysteries of cognition.},
  archiveprefix = {arXiv},
  keywords = {92-02 92B20 57Q05,Mathematics - Algebraic Topology,Quantitative Biology - Neurons and Cognition,Quantitative Biology - Quantitative Methods},
  file = {/Users/mateocastano-suarez/Zotero/storage/FWK55SPP/Giusti et al_2016_Two's company, three (or more) is a simplex.pdf;/Users/mateocastano-suarez/Zotero/storage/K8SJQC3I/1601.html}
}

