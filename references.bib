@mastersthesis{dagovic_body_2024,
  title = {Body of {{Aesthetics}}: {{The}} Therapeutic Potential of Aesthetics},
  shorttitle = {Body of {{Aesthetics}}},
  author = {Dagovic, Sara},
  year = {2024},
