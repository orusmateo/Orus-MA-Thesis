  langid = {english},
  keywords = {Computational science,Mechanical engineering},
  file = {/Users/mateocastano-suarez/Zotero/storage/L2FI95GU/Stoffel_Tandale_2024_Spiking neural networks for nonlinear regression of complex transient signals.pdf}
}

