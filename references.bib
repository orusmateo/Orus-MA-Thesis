  langid = {english},
  keywords = {Climate-change adaptation,Engineering},
  file = {/Users/mateocastano-suarez/Zotero/storage/YMJ4SSBB/Debnath et al_2023_Harnessing human and machine intelligence for planetary-level climate action.pdf}
}

