@inproceedings{1496806,
  title = {Uses and Abuses of Power in Ritual Practice with Ayahuasca},
  booktitle = {Uses and Abuses of Power in Alternative Spiritualities},
  author = {Mathiassen, Silje Trym},
  year = {2023},
  address = {Cambridge, MA},
  abstract = {{\textexclamdown}p{\textquestiondown} Abstract: {\textexclamdown}!--td border: 1px solid \#cccccc;br mso-data-placement:same-cell;--{\textquestiondown} {\textexclamdown}span{\textquestiondown}In this presentation I will show how people participating in ritual practices with ayahuasca experience transformations of power during ceremonies and in their everyday life. In my PhD-project ``Sacred plants -- spiritual experiences'' I examine how people, as part of contemporary spirituality, use ayahuasca and how this gives them experiences of both disempowerment and empowerment. Ayahuasca may cause harm, even though the plants are prescribed and used with the intention of healing or transformation. In what ways can the ritual practice be considered as abuse of power generating overwhelmingly physical and psychological reactions as well as positive use of power where the benefits sanctify the side effects?{\textexclamdown}/span{\textquestiondown} {\textexclamdown}/p{\textquestiondown} {\textexclamdown}p{\textquestiondown} Presenter bio: {\textexclamdown}!--td border: 1px solid \#cccccc;br mso-data-placement:same-cell;--{\textquestiondown} {\textexclamdown}span{\textquestiondown}Silje Trym Mathiassen is priest in The Church of Norway and has worked for several years with interreligious dialogue in Stavanger, Norway. Her special competence is within new spiritualities. Co-author of several books on interreligious dialogue and meditation, she is a PhD candidate at the University of Stavanger with the project ``Sacred plants - spiritual experiences''.{\textexclamdown}/span{\textquestiondown} {\textexclamdown}/p{\textquestiondown} {\textexclamdown}p{\textquestiondown} {\textexclamdown}span{\textquestiondown}Presenter affiliation: {\textexclamdown}/span{\textquestiondown} {\textexclamdown}!--td border: 1px solid \#cccccc;br mso-data-placement:same-cell;--{\textquestiondown} {\textexclamdown}span{\textquestiondown}University of Stavanger, Norway{\textexclamdown}/span{\textquestiondown} {\textexclamdown}/p{\textquestiondown}}
}

